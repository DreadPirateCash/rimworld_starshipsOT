<?xml version="1.0" encoding="utf-8" ?>
<Defs>

  <!-- Lambda Shuttle -->

  <ThingDef ParentName="SkyfallerBase">
    <defName>SRTSLambdaShuttle_Incoming</defName>
    <label>SRTS Lambda Shuttle incoming</label>
    <thingClass>SRTS.SRTSIncoming</thingClass>
    <graphicData>
      <texPath>Lambda</texPath>
      <graphicClass>Graphic_Multi</graphicClass>
      <shaderType>CutoutFlying</shaderType>
      <drawSize>(6,6)</drawSize>
    </graphicData>
    <skyfaller>
      <movementType>Decelerate</movementType>
      <shadow>Things/Skyfaller/SkyfallerShadowDropPod</shadow>
      <shadowSize>(6,2)</shadowSize>
      <anticipationSound>DropPod_Fall</anticipationSound>
      <anticipationSoundTicks>100</anticipationSoundTicks>
      <motesPerCell>0</motesPerCell>
    </skyfaller>
  </ThingDef>

  <ThingDef ParentName="SkyfallerBase">
    <defName>SRTSLambdaShuttle_Leaving</defName>
    <label>SRTS Lambda Shuttle leaving</label>
    <thingClass>SRTS.SRTSLeaving</thingClass>
    <graphicData>
      <texPath>Lambda</texPath>
      <graphicClass>Graphic_Multi</graphicClass>
      <shaderType>CutoutFlying</shaderType>
      <drawSize>(6,6)</drawSize>
    </graphicData>
    <skyfaller>
      <movementType>Accelerate</movementType>
      <reversed>true</reversed>
      <shadow>Things/Skyfaller/SkyfallerShadowDropPod</shadow>
      <shadowSize>(6,1)</shadowSize>
      <anticipationSound>Sound_Launch</anticipationSound>
      <anticipationSoundTicks>-10</anticipationSoundTicks>
      <ticksToImpactRange><min>-40</min><max>-15</max></ticksToImpactRange>
    </skyfaller>
  </ThingDef>

  <ThingDef>
    <defName>SRTSLambdaShuttle_Active</defName>
    <label>SRTS Lambda Shuttle active</label>
    <category>Item</category>
    <thingClass>ActiveDropPod</thingClass>
    <scatterableOnMapGen>false</scatterableOnMapGen>
    <tickerType>Normal</tickerType>
    <graphicData>
      <texPath>Lambda</texPath>
      <graphicClass>Graphic_Multi</graphicClass>
      <drawSize>(6,6)</drawSize>
    </graphicData>
    <altitudeLayer>Building</altitudeLayer>
    <blockWind>true</blockWind>
    <useHitPoints>true</useHitPoints>
    <statBases>
      <MaxHitPoints>450</MaxHitPoints>
      <Flammability>0</Flammability>
    </statBases>
    <selectable>true</selectable>
  </ThingDef>
  
